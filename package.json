{
  "name": "@moirei/vue-imager",
  "version": "1.0.0",
  "description": "Vue cloud imaging library",
  "scripts": {
    "prepare": "yarn test && yarn build",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "test:unit": "vue-cli-service test:unit",
    "test:lib": "mocha -r ts-node/register 'tests/**/*.test.ts'",
    "build:esm": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format esm",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "postbuild": "rimraf ./dist/types/dev ./dist/types/src/entry.d.ts",
    "prebuild": "rimraf ./dist",
    "prettier": "npx prettier --check src/**/*.{ts,vue}",
    "prettier:format": "npx prettier --write src/**/*.{ts,vue}",
    "test": "yarn test:lib && yarn test:unit"
  },
  "main": "dist/vue-imager.ssr.js",
  "module": "dist/vue-imager.esm.js",
  "browser": "dist/vue-imager.esm.js",
  "unpkg": "dist/vue-imager.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "types/*"
  ],
  "dependencies": {
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.mapkeys": "^4.6.7",
    "@types/lodash.omit": "^4.5.7",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.mapkeys": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "query-string": "^7.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/chai": "^4.3.1",
    "@types/lodash.isnil": "^4.0.7",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.25",
    "@types/query-string": "^6.3.0",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-plugin-unit-mocha": "~4.5.12",
    "@vue/cli-service": "^4.5.13",
    "@vue/test-utils": "^1.0.3",
    "@vuepress/plugin-active-header-links": "^1.9.7",
    "@vuepress/plugin-pwa": "^1.9.7",
    "@vuepress/plugin-register-components": "^1.9.7",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-plugin-transform-scss": "^1.1.0",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "husky": "^7.0.4",
    "lodash.isnil": "^4.0.0",
    "material-design-icons-iconfont": "^6.7.0",
    "minimist": "^1.2.5",
    "mocha": "^9.2.2",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^5.1.9",
    "sass": "~1.32.12",
    "sass-loader": "10",
    "ts-node": "^10.7.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.6.3",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vuepress": "^1.9.7",
    "vuepress-plugin-seo": "^0.2.0",
    "vuepress-plugin-typescript": "^0.3.1",
    "vuepress-theme-vt": "^0.4.2",
    "vuetify": "^2.6.4",
    "vuetify-loader": "^1.7.3"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "bugs": {
    "url": "https://github.com/moirei/vue-imager/issues"
  },
  "engines": {
    "node": ">=12"
  },
  "homepage": "https://github.com/moirei/vue-imager#readme",
  "keywords": [
    "vue",
    "cloud",
    "image",
    "media",
    "library",
    "component",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moirei/vue-imager.git"
  },
  "sideEffects": false,
  "types": "dist/types/src/entry.esm.d.ts"
}
